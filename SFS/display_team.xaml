        <Label x:Name="label2" Content="Coach&#xA;" HorizontalAlignment="Left" Margin="124,190,0,0" VerticalAlignment="Top" Height="40" FontWeight="Bold" Grid.RowSpan="2"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="195" Margin="25,221,0,0" VerticalAlignment="Top" Width="83" Grid.RowSpan="3"/>
        <ListBox x:Name="listBox1" HorizontalAlignment="Left" Height="195" Margin="118,221,0,0" VerticalAlignment="Top" Width="89" Grid.RowSpan="3"/>
        <ListBox x:Name="listBox2" HorizontalAlignment="Left" Height="195" Margin="228,221,0,0" VerticalAlignment="Top" Width="50" Grid.RowSpan="3"/>
        <Label x:Name="label3" Content="Results" HorizontalAlignment="Left" Margin="228,190,0,0" VerticalAlignment="Top" Height="26" FontWeight="Bold"/>
        <Label x:Name="label4" Content="Type&#xD;&#xA;" HorizontalAlignment="Left" Margin="294,192,0,0" VerticalAlignment="Top" Height="25" RenderTransformOrigin="0.369,0.608" FontWeight="Bold"/>
        <ListBox x:Name="listBox3" HorizontalAlignment="Left" Height="195" Margin="292,221,0,0" VerticalAlignment="Top" Width="77" Grid.RowSpan="3"/>
        <ComboBox x:Name="names" HorizontalAlignment="Left" Height="27" Margin="212,115,0,0" VerticalAlignment="Top" Width="120">
