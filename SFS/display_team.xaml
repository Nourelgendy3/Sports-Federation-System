        <ListBox x:Name="listBox4" HorizontalAlignment="Left" Height="195" VerticalAlignment="Top" Width="86" Margin="392,221,0,0" Grid.RowSpan="3"/>
        <Label x:Name="club" Content="Club&#xA;" HorizontalAlignment="Left" Margin="403,192,0,0" VerticalAlignment="Top" Height="40" FontWeight="Bold" Grid.RowSpan="2"/>
        <Button x:Name="button3" Content="" HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="45" Height="35" Margin="9,0,0,176.333" Click="button3_Click">
