        <Button x:Name="button" Content=""  BorderThickness="0" HorizontalAlignment="Left" Margin="118,141.333,0,0" VerticalAlignment="Top" Width="75" Click="button_Click" Grid.Row="2" Height="60">
            <Button.Background>
                <ImageBrush ImageSource="back.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="button1" Content="" BorderThickness="0" HorizontalAlignment="Left" Margin="265,141.333,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click" Grid.Row="2" Height="60">
